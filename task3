using System;
class Rectangle
{
    public int Width { get; set; }
    public int Height { get; set; }
    public int GetRectangleArea()
    {
        return Width * Height;
    }
}

class Square
{
    public int width { get; set; }
    public int height { get; set; }
    public int Width
    {
        get { return width; }
        set
        {
            height = value;
            width = value;
        }
    }
    public int Height
    {
        get { return height; }
        set
        {
            height = value;
            width = value;
        }
    }
    class Program
    {
        static void Main(string[] args)
        {
            Rectangle rect = new Rectangle();
            rect.Width = 5;
            rect.Height = 10;
            Console.WriteLine(rect.GetRectangleArea());
            Console.ReadKey();
        }
    }
}
